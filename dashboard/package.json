{
  "name": "dashboard-react-router-dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "backend:deno": "deno run --unstable-sloppy-imports --allow-net --allow-read --allow-ffi  --allow-env  backend/deno-serve.ts",
    "backend:d1": "wrangler dev -c backend/wrangler.toml --port 7370",
    "prebackend:pages": "pnpm run build",
    "backend:pages": "wrangler pages dev ./dist/static --live-reload",
    "predeploy:cf": "pnpm run build",
    "deploy:cf": "wrangler pages deploy ./dist/static  --commit-dirty=true",
    "build": "vite build",
    "build:tsc": "tsc",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "format": "prettier .",
    "drizzle:libsql": "drizzle-kit push --config ./drizzle.libsql.config.ts",
    "drizzle:d1-backend": "drizzle-kit push --config ./drizzle.d1-local-backend.config.ts",
    "drizzle:d1-pages": "drizzle-kit push --config ./drizzle.d1-local-pages.config.ts"
  },
  "dependencies": {
    "@adviser/cement": "^0.3.5",
    "@clerk/backend": "^1.22.0",
    "@clerk/clerk-js": "^5.40.0",
    "@clerk/clerk-react": "^5.19.0",
    "@fireproof/cloud": "^0.19.118",
    "@fireproof/core": "^0.19.121",
    "@libsql/client": "^0.14.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.1",
    "drizzle-orm": "^0.38.3",
    "highlight.js": "^11.10.0",
    "i": "^0.3.7",
    "partysocket": "^1.0.2",
    "pg": "^8.13.1",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^7.0.2",
    "react-simple-code-editor": "^0.14.1",
    "use-editable": "^2.3.3",
    "use-fireproof": "^0.19.118",
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@eslint/js": "^9.16.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.3",
    "vite": "^6.0.3"
  }
}
