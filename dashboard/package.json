{
  "name": "dashboard-react-router-dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "backend:deno": "deno run --unstable-sloppy-imports --allow-net --allow-read --allow-ffi  --allow-env --env-file=.env.local backend/deno-serve.ts",
    "backend:d1": "wrangler dev -c wrangler.toml --port 7370",
    "deploy:cf": "wrangler deploy -c wrangler.toml",
    "build": "vite build",
    "build:tsc": "tsc",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "format": "prettier .",
    "check": "pnpm format --write && tsc --noEmit && pnpm lint && pnpm test && pnpm build",
    "drizzle:libsql": "drizzle-kit push --config ./drizzle.libsql.config.ts",
    "drizzle:d1-local": "drizzle-kit push --config ./drizzle.d1-local-backend.config.ts",
    "drizzle:d1-remote": "drizzle-kit push --config ./drizzle.d1-remote.config.ts"
  },
  "dependencies": {
    "@adviser/cement": "~0.4.14",
    "@clerk/backend": "^1.25.8",
    "@clerk/clerk-js": "^5.58.1",
    "@clerk/clerk-react": "^5.25.5",
    "@fireproof/core": "^v0.21.0-dev-preview-5",
    "@monaco-editor/react": "^4.7.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.66.0",
    "highlight.js": "^11.10.0",
    "i": "^0.3.7",
    "jose": "^6.0.10",
    "minimatch": "^10.0.1",
    "multiformats": "^13.3.6",
    "partysocket": "^1.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^7.0.2",
    "react-simple-code-editor": "^0.14.1",
    "use-editable": "^2.3.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.3.0",
    "@cloudflare/workers-types": "^4.20250327.0",
    "@eslint/js": "^9.16.0",
    "@libsql/client": "^0.15.7",
    "@libsql/kysely-libsql": "^0.4.1",
    "@rollup/plugin-replace": "^6.0.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "drizzle-kit": "0.30.6",
    "drizzle-orm": "^0.40.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.1.0",
    "vitest": "^3.1.4",
    "wrangler": "^4.17.0",
    "zx": "^8.5.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "drizzle-kit": "patches/drizzle-kit.patch"
    },
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "browser-tabs-lock",
      "core-js",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
